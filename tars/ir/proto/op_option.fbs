table BatchNormOption {
  channels:int;
  slopeData:[float];
  meanData:[float];
  varData:[float];
  biasData:[float];
  Adata:[float];
  Bdata:[float];
  epsilon:float=0.001;
}

enum PadMode : byte{
    CAFFE=0,
    VALID=1,
    SAME=2
}
